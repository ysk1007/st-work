			console.log(i);
		}
		
		let arr = ['루피','조로','상디','나미'];
		for(let i = 0; i < arr.length; i++){
			console.log(arr[i]);
		}
		
		for(const i in arr){ // for(int i : arr)
			console.log(i);
		}
	</script>
