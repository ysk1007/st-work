<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

	<script>
		let carMove = function(){
			console.log('차를타고 간다');
		}
	
		let cycleMove = function(){
			console.log('자전거타고 간다');
		}
		
		let move = function (f){	// 자바 스크립트는 변수에 함수가 저장된다.
			f();	
		}
		
		let move2 = function(str){
			if(str == '차'){
				console.log('차를타고 간다');
			}
			else if(str == '자전거'){
				console.log('자전거를타고 간다');
			}
		}
		
		move(carMove);
		move(cycleMove);
		move2('차');
		move2('자전거');
	</script>
</body>
</html>