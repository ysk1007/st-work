package com.example.jpa2.entity;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import jakarta.persistence.Table;
import lombok.Data;

@Data
@Entity
@Table(name="dept")
public class Dept {
	@Id
	private int dno;
	@Column(name="dname")
	private String dname;
	@Column(name="loc")
	private String loc;
}
